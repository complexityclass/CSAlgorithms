package tests;

import sortings.QuickSort;

public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		int[] arr = new int[]{1,7,8,2,3,4,5};
		
		QuickSort qsort = new QuickSort(arr);
		qsort.print();
		System.out.println();
		qsort.sortArray();
		qsort.print();

	}

}
